<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="default" extends="struts-default">
        <action name="lookup" class="com.mushroom.action" method="lookup">
            <result>/show.jsp</result>
        </action>
        <action name="list" class="com.mushroom.action" method="list">
            <result>/index.jsp</result>
        </action>
        <action name="delete" class="com.mushroom.action" method="delete">
            <result>/index.jsp</result>
        </action>
        <action name="init" class="com.mushroom.action" method="initdb">
            <result>/index.jsp</result>
        </action>
    </package>
</struts>
